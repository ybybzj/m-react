{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "strict": 0,
    "eqeqeq": 0,
    "no-underscore-dangle": 0,
    "space-infix-ops": 0,
    "no-use-before-define": 0,
    "quotes": [
      2,
      "single"
    ]
  }
}